@model MovieRental.ViewModels.RandomMovieViewModel

<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Meu Aplicativo ASP.NET</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>

    @*Da para colocar um model tb associado a essa Partial View, usado o do Random Movie como Ex *@
    @Html.Partial("_NavBar");

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Movie Rental - Created by RomeuAC</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
